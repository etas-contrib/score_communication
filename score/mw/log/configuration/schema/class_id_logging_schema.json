
{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Traceable Objects Schema",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer"
      },
      "ctxid": {
        "type": "string"
      },
      "appid": {
        "type": "string"
      },
      "loglevel": {
        "type": "integer"
      }
    },
    "required": ["id", "ctxid", "appid", "loglevel"]
  }
}
